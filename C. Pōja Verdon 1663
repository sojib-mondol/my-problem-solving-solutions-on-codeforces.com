    #include<bits/stdc++.h>
    using namespace std;
     
    #define fl(i, n) for(int i = 0; i < n; i ++)
     
    int main() {
        int n;
        cin>>n;
        int a[n], sum = 0;
     
        fl(i, n) {
            cin>>a[i];
            sum += a[i];
        }
     
        cout<<sum<<endl;
    }
