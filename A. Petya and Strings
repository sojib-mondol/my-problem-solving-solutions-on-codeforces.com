//codeforces.com/problemset/problem/112/A
#include<bits/stdc++.h>
#include<string>

using namespace std;

int main()
{
    string a, b;
    int i;
    cin>>a>>b;
    for(i=0; i<a.size(); i ++){
        a[i] = towlower(a[i]);
        b[i] = towlower(b[i]);
    }
    if(a==b){
        cout<<0<<endl;
    }
    else{
        if(a<b){
            cout<<-1<<endl;
        }else{
            cout<<1<<endl;
        }
    }
    return 0;
}
